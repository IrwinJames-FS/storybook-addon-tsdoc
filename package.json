{
  "name": "@irwinproject/storybook-addon-tsdoc",
  "description": "Generate mdx documentation from your typescript!",
  "storybook": {
    "displayName": "@irwinproject/storybook-addon-tsdoc",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ]
  },
  "private": false,
  "dependencies": {
    "console-log-colors": "^0.5.0",
    "glob": "^11.0.0",
    "minimatch": "^10.0.1",
    "ts-morph": "^24.0.0"
  },
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp ./src/style.css ./dist/style.css",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IrwinJames-FS/storybook-addon-tsdoc"
  },
  "keywords": [
    "typescript",
    "node",
    "storybook",
    "react",
    "next",
    "documentation",
    "svelt"
  ],
  "author": "James Irwin",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "exports": {
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    }
  }
}
